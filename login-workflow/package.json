{
  "name": "@brightlayer-ui/angular-auth-workflow",
  "description": "Re-usable workflow components for Authentication and Registration within Eaton applications.",
  "version": "3.0.2",
  "scripts": {
    "ng": "ng",
    "watch": "npm-watch",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --watch false",
    "lint": "eslint \"src/**/**.ts\"",
    "lint:fix": "eslint \"src/**/**.ts\" --fix",
    "prettier": "prettier \"src/**/**.{ts,js,json,css,scss,html}\" --write",
    "prettier:check": "prettier \"src/**/**.{ts,js,json,css,scss,html}\" --check",
    "publish:package": "cd dist && set npm_config_yes=true && npx -p @brightlayer-ui/publish blui-publish",
    "tag:package": "npx -p @brightlayer-ui/tag blui-tag -s -blui-angular-auth-workflow",
    "install:dependencies": "yarn && cd example && yarn",
    "start:example": "yarn copy-to-example && cd example && yarn && yarn start:lib",
    "coverage": "yarn test --no-watch --code-coverage",
    "copy-to-example": "cp -r src/. example/src/lib/",
    "generate:licenses": "npm-license-crawler -onlyDirectDependencies -json LICENSES.json",
    "precommit": "yarn prettier && yarn lint && yarn test:ci && yarn build && yarn generate:licenses"
  },
  "prettier": "@brightlayer-ui/prettier-config",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brightlayer-ui/angular-workflows.git"
  },
  "watch": {
    "copy-to-example": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,html,scss",
      "quiet": false,
      "runOnChangeOnly": false
    }
  },
  "keywords": [
    "angular",
    "workflow",
    "authentication",
    "auth",
    "brightlayer-ui"
  ],
  "author": "brightlayer-ui <brightlayer-ui@eaton.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/brightlayer-ui/angular-workflows/issues"
  },
  "homepage": "https://github.com/brightlayer-ui/angular-workflows#readme",
  "directories": {
    "doc": "docs"
  },
  "peerDependencies": {
    "@brightlayer-ui/angular-components": ">=7.0.0",
    "@brightlayer-ui/colors": "^3.1.1"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.0",
    "@angular/animations": "^13.0.0",
    "@angular/cdk": "^13.0.0",
    "@angular/cli": "^13.0.0",
    "@angular/common": "^13.0.0",
    "@angular/compiler": "^13.0.0",
    "@angular/compiler-cli": "^13.0.0",
    "@angular/core": "^13.0.0",
    "@angular/forms": "^13.0.0",
    "@angular/material": "^13.0.0",
    "@angular/platform-browser": "^13.0.0",
    "@angular/platform-browser-dynamic": "^13.0.0",
    "@angular/router": "^13.0.0",
    "@brightlayer-ui/angular-components": "7.0.2",
    "@brightlayer-ui/colors": "^3.0.1",
    "@brightlayer-ui/eslint-config": "^3.0.1",
    "@brightlayer-ui/prettier-config": "^1.0.3",
    "@types/jasmine": "~3.9.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "codelyzer": "~5.2.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "firebase-tools": "^11.0.1",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.0.0",
    "npm-license-crawler": "^0.2.1",
    "npm-watch": "^0.11.0",
    "prettier": "^2.5.1",
    "rxjs": "~7.5.6",
    "ts-node": "~10.2.1",
    "tsickle": "0.39.1",
    "typescript": "4.4.4",
    "yarn-audit-fix": "^6.4.4",
    "zone.js": "~0.11.4"
  }
}
