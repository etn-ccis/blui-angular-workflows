{
    "name": "example",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start:lib": "ng serve --c lib",
        "build": "ng build --c production",
        "test": "ng test",
        "lint": "eslint \"src/app/**/**.ts\"",
        "e2e": "ng e2e",
        "lint:fix": "eslint \"src/app/**/**.ts\" --fix",
        "prettier": "prettier \"src/app/**/**.{ts,js,json,css,scss,html}\" --write",
        "prettier:check": "prettier \"src/app/**/**.{ts,js,json,css,scss,html}\" --check",
        "cy:run": "cypress run --config video=false",
        "cypress:open": "cypress open",
        "cytest": "start-server-and-test start http://localhost:4200 cy:run"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.2.1",
        "@angular/cdk": "^13.2.1",
        "@angular/common": "~13.2.1",
        "@angular/compiler": "~13.2.1",
        "@angular/core": "~13.2.1",
        "@angular/flex-layout": "~13.0.0-beta.38",
        "@angular/forms": "~13.2.1",
        "@angular/material": "^13.2.1",
        "@angular/platform-browser": "~13.2.1",
        "@angular/platform-browser-dynamic": "~13.2.1",
        "@angular/router": "~13.2.1",
        "@brightlayer-ui/angular-components": "^7.0.3",
        "@brightlayer-ui/angular-themes": "^7.0.0",
        "@brightlayer-ui/colors": "^3.1.1",
        "@brightlayer-ui/icons": "^1.11.0",
        "@brightlayer-ui/icons-svg": "^1.8.1",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.8"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.2.1",
        "@angular/cli": "~13.2.1",
        "@angular/compiler-cli": "~13.2.1",
        "@angular/language-service": "~13.2.1",
        "@brightlayer-ui/eslint-config": "^3.0.1",
        "@brightlayer-ui/prettier-config": "^1.0.3",
        "@cypress/code-coverage": "^3.10.0",
        "@types/jasmine": "~4.3.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^14.11.2",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.41.0",
        "codelyzer": "^6.0.2",
        "cypress": "^10.9.0",
        "cypress-localstorage-commands": "^2.2.1",
        "eslint": "^8.27.0",
        "eslint-config-prettier": "^8.5.0",
        "istanbul-instrumenter-loader": "^3.0.1",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^1.6.0",
        "ngx-build-plus": "^14.0.0",
        "prettier": "^2.5.1",
        "start-server-and-test": "^1.14.0",
        "ts-node": "~8.3.0",
        "typescript": "~4.5.4"
    },
    "prettier": "@brightlayer-ui/prettier-config",
    "nyc": {
        "exclude": [
            "src/main.ts",
            "src/app/dialog/login-error-dialog.component.ts",
            "lib/components/error-dialog/error-dialog.component.ts"
        ],
        "excludeAfterRemap": true,
        "report-dir": "../cypress-coverage"
    }
}
