{
    "name": "example",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start:lib": "ng serve --c lib  --open --host 0.0.0.0",
        "build": "ng build --c production",
        "test": "ng test",
        "lint": "eslint \"src/app/**/**.ts\"",
        "e2e": "ng e2e",
        "lint:fix": "eslint \"src/app/**/**.ts\" --fix",
        "prettier": "prettier \"src/app/**/**.{ts,js,json,css,scss,html}\" --write",
        "prettier:check": "prettier \"src/app/**/**.{ts,js,json,css,scss,html}\" --check",
        "cy:run": "cypress run --config video=false",
        "cypress:open": "cypress open",
        "cytest": "start-server-and-test start http://localhost:4200 cy:run"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^14.2.10",
        "@angular/cdk": "^14.2.7",
        "@angular/common": "^14.2.10",
        "@angular/compiler": "^14.2.10",
        "@angular/core": "^14.2.10",
        "@angular/flex-layout": "^14.0.0-beta.41",
        "@angular/forms": "^14.2.10",
        "@angular/material": "^14.2.7",
        "@angular/platform-browser": "^14.2.10",
        "@angular/platform-browser-dynamic": "^14.2.10",
        "@angular/router": "^14.2.10",
        "@brightlayer-ui/angular-components": "^8.0.0",
        "@brightlayer-ui/angular-themes": "^8.0.0",
        "@brightlayer-ui/colors": "^3.1.1",
        "@brightlayer-ui/icons": "^1.10.0",
        "@brightlayer-ui/icons-svg": "^1.11.0",
        "rxjs": "~6.6.0",
        "tslib": "^2.4.1",
        "zone.js": "~0.11.8"
    },
    "devDependencies": {
        "@angular/cli": "^14.2.9",
        "@angular/compiler-cli": "^14.2.10",
        "@angular/language-service": "^14.0.0",
        "@angular-devkit/build-angular": "^14.2.9",
        "@brightlayer-ui/eslint-config": "^3.0.0",
        "@brightlayer-ui/prettier-config": "^1.0.3",
        "@types/jasmine": "~3.9.0",
        "@types/jasminewd2": "~2.0.10",
        "@types/node": "^18.11.10",
        "@typescript-eslint/eslint-plugin": "^5.47.0",
        "@typescript-eslint/parser": "^5.42.1",
        "@cypress/code-coverage": "^3.10.0",
        "cypress": "^10.9.0",
        "cypress-localstorage-commands": "^2.2.1",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "istanbul-instrumenter-loader": "^3.0.1",
        "jasmine-core": "~4.5.0",
        "jasmine-spec-reporter": "~6.0.0",
        "karma": "^6.4.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "^2.0.3",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^1.7.0",
        "ngx-build-plus": "^14.0.0",
        "prettier": "^2.5.1",
        "start-server-and-test": "^1.14.0",
        "ts-node": "~10.2.1",
        "typescript": "4.8.4"

    },
    "prettier": "@brightlayer-ui/prettier-config",
    "nyc": {
        "exclude": [
            "src/main.ts",
            "src/app/dialog/login-error-dialog.component.ts",
            "lib/components/error-dialog/error-dialog.component.ts"
        ],
        "excludeAfterRemap": true,
        "report-dir": "../cypress-coverage"
