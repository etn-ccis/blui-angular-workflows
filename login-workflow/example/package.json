    "prettier:check": "prettier \"src/app/**/**.{ts,js,json,css,scss,html}\" --check",
    "cy:run": "cypress run --config video=false",
    "cypress:open": "cypress open",
    "cytest": "start-server-and-test start http://localhost:4200 cy:run"
